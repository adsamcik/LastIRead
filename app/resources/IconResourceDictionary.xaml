<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<DataTemplate
		x:Key="SettingsIcon">
		<Viewbox
			Stretch="Uniform">
			<Canvas
				Width="24"
				Height="24">
				<Canvas.RenderTransform>
					<TranslateTransform
						X="0"
						Y="0" />
				</Canvas.RenderTransform>
				<Canvas.Resources />
				<Ellipse
					Canvas.Left="9"
					Canvas.Top="9"
					Width="6"
					Height="6"
					StrokeThickness="2"
					Stroke="{DynamicResource SystemBaseHighColorBrush}"
					StrokeLineJoin="Round"
					StrokeStartLineCap="Round"
					StrokeEndLineCap="Round" />
				<Path
					StrokeThickness="2"
					Stroke="{DynamicResource SystemBaseHighColorBrush}"
					StrokeLineJoin="Round"
					StrokeStartLineCap="Round"
					StrokeEndLineCap="Round">
					<Path.Data>
						<PathGeometry
							Figures="m 19.4 15 a 1.65 1.65 0 0 0 0.33 1.82 l 0.06 0.06 a 2 2 0 0 1 0 2.83 2 2 0 0 1 -2.83 0 L 16.9 19.65 a 1.65 1.65 0 0 0 -1.82 -0.33 1.65 1.65 0 0 0 -1 1.51 V 21 a 2 2 0 0 1 -2 2 2 2 0 0 1 -2 -2 V 20.91 A 1.65 1.65 0 0 0 9 19.4 1.65 1.65 0 0 0 7.18 19.73 l -0.06 0.06 a 2 2 0 0 1 -2.83 0 2 2 0 0 1 0 -2.83 L 4.35 16.9 a 1.65 1.65 0 0 0 0.33 -1.82 1.65 1.65 0 0 0 -1.51 -1 H 3 a 2 2 0 0 1 -2 -2 2 2 0 0 1 2 -2 H 3.09 A 1.65 1.65 0 0 0 4.6 9 1.65 1.65 0 0 0 4.27 7.18 L 4.21 7.12 a 2 2 0 0 1 0 -2.83 2 2 0 0 1 2.83 0 L 7.1 4.35 A 1.65 1.65 0 0 0 8.92 4.68 H 9 A 1.65 1.65 0 0 0 10 3.17 V 3 a 2 2 0 0 1 2 -2 2 2 0 0 1 2 2 v 0.09 a 1.65 1.65 0 0 0 1 1.51 1.65 1.65 0 0 0 1.82 -0.33 l 0.06 -0.06 a 2 2 0 0 1 2.83 0 2 2 0 0 1 0 2.83 L 19.65 7.1 A 1.65 1.65 0 0 0 19.32 8.92 V 9 a 1.65 1.65 0 0 0 1.51 1 H 21 a 2 2 0 0 1 2 2 2 2 0 0 1 -2 2 h -0.09 a 1.65 1.65 0 0 0 -1.51 1 z"
							FillRule="NonZero" />
					</Path.Data>
				</Path>
			</Canvas>
		</Viewbox>

	</DataTemplate>

	<DataTemplate
		x:Key="FilterIcon">
		<Viewbox
			Stretch="Uniform">
			<Canvas
				Width="24"
				Height="24">
				<Canvas.RenderTransform>
					<TranslateTransform
						X="0"
						Y="0" />
				</Canvas.RenderTransform>
				<Canvas.Resources />
				<Polygon
					Points="22 3 2 3 10 12.46 10 19 14 21 14 12.46 22 3"
					Name="polygon843"
					FillRule="NonZero"
					StrokeThickness="2"
					Stroke="{DynamicResource SystemBaseHighColorBrush}"
					StrokeLineJoin="Round"
					StrokeStartLineCap="Round"
					StrokeEndLineCap="Round" />
			</Canvas>
		</Viewbox>
	</DataTemplate>

